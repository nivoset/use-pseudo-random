{
  "name": "use-pseudo-random",
  "version": "1.1.7",
  "description": "a library for using a pseudo random number generator in react hooks",
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "types": "./lib/esm/index.d.ts",
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm",
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --outDir lib/cjs",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:coverage": "jest --no-watch --coverage"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "prando": "^6.0.1"
  },
  "peerDependencies": {
    "react": "^17.0.11"
  },
  "devDependencies": {
    "@testing-library/react-hooks": "^7.0.2",
    "@types/jest": "^27.0.3",
    "@types/prando": "^1.0.0",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "jest": "^27.3.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hooks-testing-library": "^0.6.0",
    "ts-jest": "^27.0.7",
    "typescript": "^4.5.2"
  }
}
